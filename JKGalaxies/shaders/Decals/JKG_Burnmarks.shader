gfx/burnmarks/thermalcrater_glow1
{
	qer_editorimage	gfx/burnmarks/thermalcrater_glow1
	polygonOffset
	q3map_nolightmap
	sort	decal
	deformvertexes	bulge	0 0.1 0
    {
        clampMap  gfx/burnmarks/thermalcrater_glow1
        blendFunc GL_ONE GL_ONE
        glow
        detail
        rgbGen vertex
//        rgbGen wave sin 1 0.5 0.3 0.15
        tcMod stretch sin 1 0.01 0 0.2
    }
}

