// Icon replacements for the weapon pickups, somewhat corresponding to the different weapon classes.

// Array Guns

gfx/hud/w_icon_flechette
{
	nomipmaps
	cull	twosided
    {
        ClampMap models/weapons/ACP_ArrayGun/icon_wire
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen vertex
    }
}

gfx/hud/w_icon_flechette_na
{
	nomipmaps
	cull	twosided
    {
        ClampMap models/weapons/ACP_ArrayGun/icon_wire
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen const ( 0.000000 0.000000 0.000000 )
    }
}

